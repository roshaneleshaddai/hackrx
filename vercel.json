{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 30
    }
  },
  "ignore": [
    "myenv/**",
    "*.pyc",
    "__pycache__/**",
    ".env",
    ".git/**",
    "*.log"
  ]
}
  